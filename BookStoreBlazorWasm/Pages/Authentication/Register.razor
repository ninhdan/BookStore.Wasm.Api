@page "/Register"
@using BookStoreBlazorWasm.Services.Interfaces
@using BookStoreView.Models.Dtos.DtoUser
@layout LayoutAuthentication
@inherits RegisterUserBase



<style>
    .validation-message{
        font-size: 13px; 
        margin-bottom: 5px;
    }

    .custom-input{
        width: 60%;
        height: 35px;
        font-size: 14px;
    }


</style>

<RadzenRow Gap="0" Class="rz-my-12 rz-mx-auto rz-border-radius-6 rz-shadow-10" Style="width: 100%; max-width:1000px; overflow: hidden;">

    <RadzenColumn Size="12" SizeMD="6">
        <RadzenCard Class="rz-shadow-0 rz-border-radius-0 rz-p-12">
            <RadzenText TextStyle="TextStyle.H5" TagName="TagName.H2" Class="rz-mb-6">
                Đăng ký
            </RadzenText>
             <EditForm Model="@newUser" OnValidSubmit="RegisterUser"> 

                <DataAnnotationsValidator />
             <div style="display: flex; flex-direction: column; align-items: flex-end;">
                    <div class="form-group mb-3" style="position: relative; width: 100%; display: flex; justify-content: space-between; align-items: center;">
                        <RadzenLabel Component="firstnameInput" Text="Họ" Style="margin-right: 16px; " />
                        <RadzenTextBox @bind-Value="newUser.FirstName" class="custom-input" Placeholder="Nhập họ .." />

                        <ValidationMessage For="@(() => newUser.FirstName)" Class="validation-message" style="position: absolute; top: 100%; left: 0; width: 100%;" />
                    </div>
                    <div class="form-group mb-3" style="position: relative; width: 100%; display: flex; justify-content: space-between; align-items: center;">
                        <RadzenLabel Component="lastnameInput" Text="Tên" Style="margin-right: 16px; " />
                        <RadzenTextBox @bind-Value="newUser.LastName" Placeholder="Nhập tên .." class="custom-input" />
                        <ValidationMessage For="@(() => newUser.LastName)" Class="validation-message" style="position: absolute; top: 100%; left: 0; width: 100%;" />
                    </div>

                    <div class="form-group mb-3" style="position: relative; width: 100%; display: flex; justify-content: space-between; align-items: center;">
                        <RadzenLabel Component="PhoneInput" Text="Số điện thoại" Style="margin-right: 16px; "   />
                        <RadzenTextBox @bind-Value="newUser.Phone"  Placeholder="Nhập số điện thoại .." class="custom-input" />
                        <ValidationMessage For="@(() => newUser.Phone)" Class="validation-message" style="position: absolute; top: 100%; left: 0; width: 100%;" />
                    </div>
                 

                    <div class="form-group mb-3" style="position: relative; width: 100%; display: flex; justify-content: space-between; align-items: center;">
                        <RadzenLabel Component="passwordInput" Text="Mật khẩu" Style="margin-right: 16px;"  />
                        <RadzenPassword @bind-Value="newUser.Password"  Placeholder="Nhập mật khẩu .." class="custom-input" />
                        <ValidationMessage For="@(() => newUser.Password)" class="validation-message" style="position: absolute; top: 100%; left: 0; width: 100%;" />
                </div>
                
                    <div class="form-group mb-3" style="position: relative; width: 100%; display: flex; justify-content: space-between; align-items: center;">
                        <RadzenLabel Component="confirmPasswordInput" Text="Xác nhận mật khẩu" Style="margin-right: 16px;" />
                        <RadzenPassword @bind-Value="newUser.ConfirmPassword" Placeholder="Xác nhận lại mật khẩu .." class="custom-input" />
                        <ValidationMessage For="@(() => newUser.ConfirmPassword)" class="validation-message" style="position: absolute; top: 100%; left: 0; width: 100%;" />

                </div>
                  
             </div>
                <div class="form-actions " style="display: flex; justify-content: flex-end; margin-top: 16px;">
                    <RadzenButton Variant="Variant.Flat" ButtonType="ButtonType.Submit" Shade="Shade.Lighter" Text="Đăng ký" ButtonStyle="ButtonStyle.Info" Style="width: auto; margin-right: 8px;" />

                    <RadzenButton Variant="Variant.Flat" Shade="Shade.Lighter" Text="Quay lại" ButtonStyle="ButtonStyle.Danger" Click="GotoBackLogin" />
                </div>

             </EditForm> 
               
          
        </RadzenCard>
    </RadzenColumn>


    <RadzenColumn Size="12" SizeMD="6">
        <RadzenCard Class="rz-shadow-0 rz-border-radius-0 rz-text-align-center rz-p-12" Style="height: 100%; background:  no-repeat 100% 20% fixed url('data:image/jpeg;base64,')">
            <RadzenText TextStyle="TextStyle.DisplayH3" TagName="TagName.H2" Style="color: #001836" Class=" rz-mb-6">Welcome! </RadzenText>

            <RadzenImage Path="images/logo/Logo-Remove.png" Style="max-width: 300px; max-height: 300px;" Class="rz-mb-4" />

            <RadzenText TextStyle="TextStyle.H6" Class="rz-color-white"> <span style=""></span>Water 7 <span style="color:#001836">BookStore</span> </RadzenText>
            <RadzenText TextStyle="TextStyle.Body2" Class="rz-color-white">
                <strong style="color: #03AEDE">"Access Knowledge, Open Windows to the Soul."</strong>
            </RadzenText>
            <br />
            <RadzenText TextStyle="TextStyle.Body2" Class="rz-color-white">
               
            </RadzenText>
        </RadzenCard>
    </RadzenColumn>
</RadzenRow>


